#include<stdio.h>
void main()
{
	int m;
	printf("Bit Elementary School.....");
	printf("\nEnter Marks obtained- ");
	scanf("%d",&m);
	printf("\n\nStudent Obtained - ");
	if(m>=85 && m<=100)
	   printf("Gade A");
	if(m>=70 && m<=84)
	   printf("Grade B");
	if(m>=55 && m<=69)
	   printf("Grade C");
	if(m>=40 && m<=54)
	   printf("Grade D");	
	if(m<40)
	   printf("Grade F");  
	getch();
}
